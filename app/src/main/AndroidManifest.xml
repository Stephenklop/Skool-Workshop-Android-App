<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.skoolworkshop2">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SkoolWorkshop2">
        <activity android:name=".WorkshopActivity"></activity>
        <activity android:name=".ui.WorkshopDetail.WorkshopDetailActivity"></activity>
        <activity android:name=".ui.CulturedayActivity"></activity>
        <activity android:name=".ui.CulturedayBookingActivity"></activity>
        <activity android:name=".ui.CulturedayQuestionActivity"></activity>
        <activity android:name=".ui.WorkshopDetail.WorkshopQuestionActivity"></activity>
        <activity android:name=".ui.WorkshopDetail.WorkshopBookingActivity"></activity>
        <activity
            android:name=".ui.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ShoppingCartLayoutTestActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddressInfoLayoutTestActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>